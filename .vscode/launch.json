  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: process_reviews cluster",
      "type": "debugpy",
      "request": "launch",
      "module": "src.review_clusterer.cli",
      "args": ["index", "~/temp/reviews.csv", "--local"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${command:python.interpreterPath}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
